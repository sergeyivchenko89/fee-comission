{
    "name": "sergei-ivchenko/commission-task-skeleton",
    "description": "Comission task skeleton",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "SergeiIvchenko\\CommissionTask\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SergeiIvchenko\\CommissionTask\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-readline": "*",
        "php-di/php-di": "^6.3",
        "psr/cache": "^1.0",
        "symfony/cache": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.16",
        "phpunit/phpunit": "^6.5",
        "symfony/dotenv": "^5.3"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    }
}
